<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.main.pj.LoginMapper">

	<select id="getShainnByID" parameterType="com.main.pj.Shainn_info" resultType="com.main.pj.Shainn_info">
		select * from 
			shainn_table 
		where shainn_number = #{shainn_number}
	</select>

</mapper>
